   "source": [
    "# Write a Python function to reverse a given string using slicing."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "f0ce8ac8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a string: zaiaf yeh\n",
      "Reversed string: hey faiaz\n"
     ]
    }
   ],
   "source": [
    "def reverse_string():\n",
    "    strg = input(\"Enter a string: \")\n",
    "    rev_strg = strg[::-1]\n",
    "    print(\"Reversed string:\", rev_strg)\n",
    "reverse_string()"
