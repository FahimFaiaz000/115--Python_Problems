   "id": "7b9e0616",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a list of integers: 5 4 6 7 9 \n",
      "The maximum value is: 9 & the minimum is: 4\n"
     ]
    }
   ],
   "source": [
    "num_list = input(\"Enter a list of integers: \").split()\n",
    "num_list = [int(x) for x in num_list]\n",
    "\n",
    "print(f\"The maximum value is: {max(num_list)} & the minimum is: {min(num_list)}\")"
   ]
