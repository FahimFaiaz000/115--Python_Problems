   "source": [
    "# Given a list of nested lists, write a Python program to concatenate all the sublists into a single flat list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "c05b525a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a list of nested lists: [[1, 2], [3, 4]]\n",
      "Flattened list: [1, 2, 3, 4]\n"
     ]
    }
   ],
   "source": [
    "nested_lists = eval(input(\"Enter a list of nested lists: \"))\n",
    "\n",
    "flat_list = [item for sublist in nested_lists for item in sublist]\n",
    "\n",
    "print(\"Flattened list:\", flat_list)"
   ]
  }
